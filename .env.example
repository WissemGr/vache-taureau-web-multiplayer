# Variables d'environnement
PORT=3000
NODE_ENV=production

# Configuration optionnelle
MAX_PLAYERS_PER_ROOM=8
ROOM_CLEANUP_INTERVAL=300000
AUTO_START_DELAY=5000

# Sécurité des sessions
SESSION_SECRET=change-this-secret-key-in-production

# Upstash Redis (for persistent storage on Vercel)
# Get these from https://console.upstash.com/
# Leave empty for local development (will use in-memory storage)
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=
